FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      rho2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -3 0 0 0 0 0];

internalField   uniform 1.293;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    wedges
    {
        type            symmetry;
    }
    centre
    {
        type            zeroGradient;
    }
}


